{
	"name": "@lk/core-utils",
	"version": "4.6.48",
	"description": "Core Utility package of lk",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"browser": "dist/index.bundle.js",
	"typings": "dist/index.d.ts",
	"scripts": {
		"clean": "rimraf dist",
		"lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
		"prepare": "husky install",
		"build:pkg": "yarn clean && rollup -c",
		"start": "rollup -c -w --environment BUILD:development",
		"commit": "cz",
		"test": "jest",
		"publish": "yarn clean && rollup -c && npm publish"
	},
	"repository": {
		"type": "git",
		"url": "http://gitlab.lenskartserver.net/phoenix/infra/core-utils.git"
	},
	"files": [
		"dist",
		"README.md"
	],
	"author": "Lenskart",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@babel/preset-env": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@commitlint/cli": "^17.0.2",
		"@commitlint/config-conventional": "^17.0.2",
		"@rollup/plugin-commonjs": "^22.0.1",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@types/node": "^18.0.3",
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"babel-jest": "^28.1.2",
		"commitizen": "^4.2.4",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^16.0.1",
		"eslint": "^8.17.0",
		"eslint-plugin-import": "^2.26.0",
		"husky": "^8.0.1",
		"jest": "^28.1.2",
		"jest-environment-jsdom": "^28.1.2",
		"prettier": "^2.6.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.75.5",
		"rollup-plugin-filesize": "^9.1.2",
		"rollup-plugin-livereload": "^2.0.5",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-serve": "^1.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-ts": "^3.0.1",
		"standard-version": "^9.5.0",
		"typescript": "^4.7.3"
	},
	"directories": {
		"dist": "dist"
	},
	"publishConfig": {
		"registry": "https://npm-preprod.lenskart.com/"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@lk/utils": "^0.0.6"
	}
}
